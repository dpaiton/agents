# deployment/gcp/terraform.tfvars.example â€” Copy to terraform.tfvars and fill in.
#
# Usage:
#   cp terraform.tfvars.example terraform.tfvars
#   $EDITOR terraform.tfvars
#   terraform init && terraform apply

project_id   = "my-gcp-project"
region       = "us-central1"
zone         = "us-central1-a"
machine_type = "e2-standard-2"
disk_size_gb = 30
preemptible  = true

# Optional: set to enable log archival to GCS
# log_bucket_name = "my-project-agents-logs"

